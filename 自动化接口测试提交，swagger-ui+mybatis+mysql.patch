@@ -0,0 +1,27 @@
+package com.course.model;
+
+import lombok.Data;
+
+@Data
+public class User {
+    private int id;
+    private String userName;
+    private String password;
+    private String age;
+    private String sex;
+    private String isDelete;
+    private String permission;
+
+    @Override
+    public String toString(){
+        return(
+                "{id:"+id+","+
+                "userName:"+userName+","+
+                "password:"+password+","+
+                "age:"+age+","+
+                "sex:"+sex+","+
+                "isDelete:"+isDelete+","+
+                "permission:"+permission+"}"
+                );
+    }
+}
